{
    "version": 2,
    "buildCommand": "npm install -g pnpm@9 && pnpm install --no-frozen-lockfile && pnpm --filter @remote-app/shared build && pnpm --filter @remote-app/client build",
    "outputDirectory": "packages/client/dist",
    "installCommand": "npm install -g pnpm@9 && pnpm install --no-frozen-lockfile",
    "framework": null,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "env": {
        "VITE_SERVER_URL": "https://kodokremote.zeabur.app"
    }
}